<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.gracecode.apcupsd_guarder</string>
        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/bin/apcupsd_guarder</string>
        </array>
        <key>KeepAlive</key>
        <dict>
            <key>NetworkState</key>
            <false/>
            <key>Crashed</key>
            <true/>
            <key>SuccessfulExit</key>
            <false/>
        </dict>
        <key>RunAtLoad</key>
        <true/>
        <key>ProcessType</key>
        <string>Background</string>
        <key>StandardErrorPath</key>
        <string>/usr/local/var/log/apcupsd_guarder.log</string>
        <key>StandardOutPath</key>
        <string>/usr/local/var/log/apcupsd_guarder.log</string>
    </dict>
</plist>
